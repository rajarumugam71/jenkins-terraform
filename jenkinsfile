
pipeline{
  agent any
  environment {
  PATH = "${PATH}/terraform"

}
  stages{
    stage('terraform init'){

      steps{
           sh 'pwd'
           
           sh "terraform init"

      }
    }

  }

}

def getTerraformPath(){

   def   tfHome =  tool name: 'terraform-12', type: 'terraform'

   echo tfHome
   return tfHome
}
